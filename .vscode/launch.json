  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/srv",
      "outFiles": [ "${workspaceFolder}/build/**/*.js" ],
      "protocol": "inspector",
      "sourceMaps": true,
      "name": "Attach",
      "port": 9229,
      "restart": true,
      "smartStep": true,
      "trace": "sm"
    }
  ]
}
